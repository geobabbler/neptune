{
    "name": "neptune",
    "version": "1.1.0",
    "description": "A Node.js application that aggregates RSS/Atom feeds from an OPML file and generates a list of feeds, an aggregated HTML page, and RSS feed. Trans rights are human rights.",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "start:https": "node app.js --https",
        "go": "node app.js",
        "go:https": "node app.js --https",
        "test:random-feeds": "node app.js --random-feeds=10",
        "generate-cert": "node generate-cert.js"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.4",
        "axios": "^1.3.4",
        "cheerio": "^1.0.0-rc.12",
        "ejs": "^3.1.10",
        "express": "^4.18.2",
        "html-entities": "^2.4.0",
        "marked": "^17.0.3",
        "node-schedule": "^2.1.0",
        "rss": "^1.2.2",
        "xml2js": "^0.5.0"
    },
    "author": "Bill Dollins",
    "email": "bill@cercanasystems.com",
    "homepage": "https://cercanasystems.com",
    "license": "MIT"
}
